    "/v1/device_models/list": {
      "get": {
        "x-fern-sdk-group-name": ["v1", "device_models"],
        "x-fern-sdk-method-name": "list",
        "summary": "/v1/device_models/list",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "device_models": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "device_model_id": {
                            "type": "string",
                            "format": "uuid"
                          },
                          "manufacturer": {
                            "type": "object",
                            "properties": {
                              "manufacturer_id": {
                                "type": "string",
                                "format": "uuid"
                              },
                              "display_name": { "type": "string" },
                              "logo": {
                                "type": "object",
                                "properties": {
                                  "url": { "type": "string", "format": "uri" },
                                  "width": { "type": "number" },
                                  "height": { "type": "number" }
                                },
                                "required": ["url", "width", "height"]
                              },
                              "integration": {
                                "type": "string",
                                "enum": [
                                  "stable",
                                  "beta",
                                  "planned",
                                  "unsupported"
                                ]
                              },
                              "is_connect_webview_supported": {
                                "type": "boolean"
                              },
                              "requires_seam_support_to_add_account": {
                                "type": "boolean"
                              }
                            },
                            "required": [
                              "manufacturer_id",
                              "display_name",
                              "integration",
                              "is_connect_webview_supported",
                              "requires_seam_support_to_add_account"
                            ]
                          },
                          "is_device_supported": { "type": "boolean" },
                          "display_name": { "type": "string" },
                          "product_url": { "type": "string" },
                          "main_connection_type": {
                            "type": "string",
                            "enum": ["wifi", "zwave", "zigbee", "unknown"]
                          },
                          "main_category": {
                            "type": "string",
                            "enum": ["smartlock", "thermostat", "noise_sensor"]
                          },
                          "aesthetic_variants": {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "slug": { "type": "string" },
                                "display_name": { "type": "string" },
                                "primary_color_hex": { "type": "string" },
                                "manufacturer_sku": { "type": "string" },
                                "front_image": {
                                  "type": "object",
                                  "properties": {
                                    "url": {
                                      "type": "string",
                                      "format": "uri"
                                    },
                                    "width": { "type": "number" },
                                    "height": { "type": "number" }
                                  },
                                  "required": ["url", "width", "height"]
                                },
                                "back_image": {
                                  "type": "object",
                                  "properties": {
                                    "url": {
                                      "type": "string",
                                      "format": "uri"
                                    },
                                    "width": { "type": "number" },
                                    "height": { "type": "number" }
                                  },
                                  "required": ["url", "width", "height"]
                                }
                              },
                              "required": [
                                "slug",
                                "display_name",
                                "manufacturer_sku"
                              ]
                            }
                          }
                        },
                        "required": [
                          "device_model_id",
                          "manufacturer",
                          "is_device_supported",
                          "display_name",
                          "product_url",
                          "main_connection_type",
                          "main_category",
                          "aesthetic_variants"
                        ]
                      }
                    },
                    "ok": { "type": "boolean" }
                  },
                  "required": ["device_models", "ok"]
                }
              }
            }
          },
          "400": { "description": "Bad Request" },
          "401": { "description": "Unauthorized" }
        },
        "tags": [],
        "operationId": "v1DeviceModelsListGet"
      }
    }
  },
  "components": {
    "securitySchemes": { "api_key": { "type": "apiKey" } },
    "schemas": {}
